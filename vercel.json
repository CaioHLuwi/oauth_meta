{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/oauth_meta/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/oauth_meta/oauth/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/oauth_meta/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/meta-ads/(.*)",
      "dest": "/api/index.js"
    }
  ]
}

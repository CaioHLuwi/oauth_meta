{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/oauth_meta/oauth/initiate",
      "dest": "/api/index.js"
    },
    {
      "src": "/oauth_meta/oauth-callback.html",
      "dest": "/public/oauth-callback.html"
    },
    {
      "src": "/oauth_meta/oauth-error.html",
      "dest": "/public/oauth-error.html"
    },
    {
      "src": "/oauth_meta/oauth-callback.js",
      "dest": "/public/oauth-callback.js"
    },
    {
      "src": "/oauth_meta/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/meta-ads/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
